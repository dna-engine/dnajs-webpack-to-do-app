{
   "name": "dnajs-webpack-to-do-app",
   "description": "Sample To Do application powered by dna.js and built with webpack",
   "version": "1.0.0",
   "license": "MIT",
   "main": "app.js",
   "repository": "github:dnajs/dnajs-webpack-to-do-app",
   "homepage": "https://github.com/dnajs/dnajs-webpack-to-do-app",
   "bugs": "https://github.com/dnajs/dnajs-webpack-to-do-app/issues",
   "docs": "https://dnajs.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "dnajs",
      "webpack",
      "todo",
      "nodejs",
      "javascript"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "browser": true,
      "node": true
   },
   "scripts": {
      "test": "jshint *.js src && rimraf dist **/.DS_Store && webpack",
      "start": "npm test && open dist/index.html"
   },
   "dependencies": {
      "dna.js": "~1.7",
      "jquery": "~3.6"
   },
   "devDependencies": {
      "css-loader": "~5.2",
      "html-webpack-plugin": "~5.3",
      "jshint": "~2.13",
      "rimraf": "~3.0",
      "style-loader": "~2.0",
      "webpack": "~5.39",
      "webpack-cli": "~4.7"
   }
}
