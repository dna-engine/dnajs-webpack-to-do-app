{
   "name": "dnajs-webpack-to-do-app",
   "description": "Sample To Do application powered by dna.js and built with webpack",
   "version": "1.0.0",
   "license": "MIT",
   "main": "app.js",
   "repository": "dnajs/dnajs-webpack-to-do-app",
   "homepage": "https://github.com/dnajs/dnajs-webpack-to-do-app",
   "bugs": "https://github.com/dnajs/dnajs-webpack-to-do-app/issues",
   "docs": "http://dnajs.org/docs",
   "keywords": [
      "dnajs",
      "webpack",
      "todo",
      "nodejs",
      "javascript"
   ],
   "author": {
      "name": "Center Key",
      "url": "http://centerkey.com"
   },
   "scripts": {
      "pretest": "webpack",
      "test": "jshint *.js src/js/*.js"
   },
   "jshintConfig": {
      "esversion": 6,
      "strict": "implied",
      "node": true,
      "undef": true,
      "unused": true,
      "globals": {
         "window": false
      }
   },
   "dependencies": {
      "dna.js": "~1.3"
   },
   "devDependencies": {
      "css-loader": "~0.28",
      "extract-text-webpack-plugin": "~3.0",
      "html-webpack-plugin": "~2.30",
      "jshint": "~2.9",
      "raw-loader": "~0.5",
      "webpack": "~3.5"
   }
}
