{
   "name": "dnajs-webpack-to-do-app",
   "description": "Sample To Do application powered by dna.js and built with webpack",
   "version": "1.0.0",
   "license": "MIT",
   "main": "src/js/app.js",
   "repository": "github:dnajs/dnajs-webpack-to-do-app",
   "homepage": "https://github.com/dnajs/dnajs-webpack-to-do-app",
   "bugs": "https://github.com/dnajs/dnajs-webpack-to-do-app/issues",
   "docs": "https://dnajs.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "dnajs",
      "webpack",
      "todo",
      "nodejs",
      "javascript"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "browser": true,
      "mocha": true,
      "node": true
   },
   "scripts": {
      "pretest": "jshint . --exclude node_modules,dist && rimraf dist **/.DS_Store && webpack",
      "test": "mocha spec/*.spec.mjs",
      "start": "npm test && open dist/index.html"
   },
   "dependencies": {
      "dna.js": "~2.0",
      "jquery": "~3.6"
   },
   "devDependencies": {
      "assert-deep-strict-equal": "~1.0",
      "css-loader": "~6.7",
      "html-webpack-plugin": "~5.5",
      "jshint": "~2.13",
      "mocha": "~10.0",
      "rimraf": "~3.0",
      "style-loader": "~3.3",
      "webpack": "~5.73",
      "webpack-cli": "~4.10"
   }
}
