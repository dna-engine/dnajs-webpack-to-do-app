{
   "name": "webpack-to-do-app",
   "description": "Sample To Do application powered by dna-engine and built with webpack",
   "version": "1.0.0",
   "license": "MIT",
   "main": "src/js/app.js",
   "repository": "github:dna-engine/webpack-to-do-app",
   "homepage": "https://github.com/dna-engine/webpack-to-do-app",
   "bugs": "https://github.com/dna-engine/webpack-to-do-app/issues",
   "docs": "https://dna-engine.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "dna-engine",
      "webpack",
      "todo",
      "nodejs",
      "javascript"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "browser": true,
      "mocha": true,
      "node": true
   },
   "scripts": {
      "pretest": "jshint . --exclude-path .gitignore && rimraf dist && webpack",
      "test": "mocha spec/*.spec.mjs",
      "start": "npm test && open dist/index.html"
   },
   "dependencies": {
      "dna-engine": "~2.3",
      "jquery": "~3.6"
   },
   "devDependencies": {
      "assert-deep-strict-equal": "~1.0",
      "css-loader": "~6.7",
      "html-webpack-plugin": "~5.5",
      "jshint": "~2.13",
      "mocha": "~10.2",
      "rimraf": "~4.4",
      "style-loader": "~3.3",
      "webpack": "~5.76",
      "webpack-cli": "~5.0"
   }
}
